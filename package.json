{
  "name": "ultimate-react-ts-vite-starter-kit",
  "version": "1.0.0",
  "description": "ModÃ¨le de base pour commencer un projet avec Husky, ESLint, Prettier, Typescript React Vite et Vitest",
  "main": "index.js",
  "scripts": {
    "docker:start:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:stop:dev": "docker-compose -f docker-compose.dev.yml down -v",
    "docker:start:prod": "docker-compose -f docker-compose.prod.yml up --build",
    "docker:stop:prod": "docker-compose -f docker-compose.prod.yml down -v",
    "start": "vite",
    "start:host": "vite --host",
    "build": "tsc --project tsconfig.prod.json && vite build --outDir ./dist",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --fix",
    "lint:scss": "npx stylelint **/*.scss --max-warnings 0 --fix",
    "format": "prettier --write",
    "prepare": "husky install"
  },
  "pre-commit": "lint-staged",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mehomestudio/Ultimate-React-TS-Vite-starter-kit.git"
  },
  "keywords": [
    "typescript",
    "react",
    "vite",
    "vitest",
    "eslint",
    "prettier",
    "husky",
    "conventionnal-commit"
  ],
  "author": "mhjdev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mehomestudio/Ultimate-React-TS-Vite-starter-kit/issues"
  },
  "homepage": "https://github.com/mehomestudio/Ultimate-React-TS-Vite-starter-kit#readme",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/eslint": "^8.4.6",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vitest/coverage-c8": "^0.29.8",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "stylelint": "^15.6.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-order": "^6.0.2"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.0",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.11",
    "cssnano": "^6.0.1",
    "cssnano-preset-default": "^5.2.12",
    "postcss": "^8.4.16",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-sort-media-queries": "^4.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.35.0",
    "react-router-dom": "^6.4.0",
    "sass": "^1.54.9",
    "sass-swing": "^1.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.29.2",
    "yup": "^0.32.11"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@testing-library/dom",
        "eslint-plugin-react-hooks",
        "eslint-plugin-react",
        "eslint-plugin-prettier",
        "eslint-plugin-jsx-a11y",
        "eslint-config-standard",
        "eslint-plugin-n",
        "eslint-plugin-promise",
        "@testing-library/user-even"
      ]
    }
  }
}
